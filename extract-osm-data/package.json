{
  "name": "@public-transit-detector/extract-osm-data",
  "version": "0.0.1",
  "description": "Extracts public transit data from a PBF file and writes it to a CSV file",
  "main": "dist/extract-osm-data.js",
  "bin": "dist/extract-osm-data.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/extract-osm-data.js",
      "require": "./dist/extract-osm-data.umd.cjs"
    },
    "./dist/index.d.ts": "./dist/index.d.ts",
    "./dist/extract-osm-data.js": "./dist/extract-osm-data.js",
    "./dist/extract-osm-data.umd.cjs": "./dist/extract-osm-data.umd.cjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/public-transit-detector/public-transit-detector.git"
  },
  "keywords": [
    "public transit",
    "transit",
    "location",
    "location services",
    "gps"
  ],
  "author": "Tim Vahlbrock",
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/public-transit-detector/public-transit-detector/issues"
  },
  "homepage": "https://github.com/public-transit-detector/public-transit-detector/extract-osm-data/#readme",
  "scripts": {
    "build": "rm -rf dist && vite build",
    "lint": "eslint \"./**/*.ts\" --fix",
    "test": "vitest"
  },
  "dependencies": {
    "osm-pbf-parser-node": "^1.1.3",
    "pako": "^2.1.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^20.9.0",
    "@types/pako": "^2.0.3",
    "@types/yargs": "^17.0.32",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "csv-stringify": "^6.4.5",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.29.0",
    "vite": "^5.1.1",
    "vitest": "^1.1.3"
  }
}
